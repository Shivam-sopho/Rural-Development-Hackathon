<!DOCTYPE html>
<head>
	<title>MAPS</title>
	<script type="text/javascript">
	</script>
</head>
<body>
	<script>
      // Note: This example requires that you consent to location sharing when
      // prompted by your browser. If you see the error "The Geolocation service
      // failed.", it means you probably did not give permission for the browser to
      // locate you.
      var map, infoWindow;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -34.397, lng: 150.644},
          zoom: 6
        });
        infoWindow = new google.maps.InfoWindow;

        var lat,lng;
        // Try HTML5 geolocation.
        if (navigator.geolocation) {
          navigator.geolocation.getCurrentPosition(function(position) {
            var lat;
            var lng;
            var pos = {
              lat: position.coords.latitude,
              lng: position.coords.longitude,

              //lat=lat;
              //lng=lng;
             // var temp="https://maps.googleapis.com/maps/api/geocode/json?latlng=lat,lng&key=AIzaSyDL-fdxcM_dU6W5udqLnGeceFJ7F_tQt2c"
            };
            lat=pos.lat;
            lng=pos.lng;
            
           //var temp="https://maps.googleapis.com/maps/api/geocode/json?latlng=lat,lng&key=AIzaSyDL-fdxcM_dU6W5udqLnGeceFJ7F_tQt2c"
          }, function() {
            handleLocationError(true, infoWindow, map.getCenter());
          });
        } else {
          // Browser doesn't support Geolocation
          handleLocationError(false, infoWindow, map.getCenter());
        }
      }

      function handleLocationError(browserHasGeolocation, infoWindow, pos) {
        infoWindow.setPosition(pos);
        infoWindow.setContent(browserHasGeolocation ?
                              'Error: The Geolocation service failed.' :
                              'Error: Your browser doesn\'t support geolocation.');
        //infoWindow.open(map);
      }

    </script>
    <script async defer
    	src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD18to1SISoABV2pzXMH79XK0sJ3hUteQA
		&callback=initMap">
    </script>
	<iframe
  			width="1350"
  			height="600"
  			frameborder="0" style="border:0"
  			onload="this.src='https://www.google.com/maps/embed/v1/directions?key=AIzaSyD18to1SISoABV2pzXMH79XK0sJ3hUteQA&origin='+lat+' '+lng+'lng&destination=<%= nodal[0].nodalCenter%>'"
  		 allowfullscreen>
	</iframe>
</body>
<script>

</script>
</html>